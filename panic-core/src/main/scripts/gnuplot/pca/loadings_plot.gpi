#!/usr/bin/env gnuplot


set terminal postscript eps enhanced color font 'Arial,12'

set title "Loadings plot"
set xlabel "PC1"
set ylabel "PC2"
unset key
set grid
set macro


set output filename.".eps"
plot filename u 2:3:1 index 0 w labels point ps 4 pt 6 offset -.25,-.25

#set title "Scores plot"
#set output "scores.eps"
#plot filename u 1:2 index 1 w point ps 3 pt 5


system("epstopdf ".filename.".eps")
#system("epstopdf scores.eps")
system("rm *.eps")
